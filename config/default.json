{
  "server": {
    "host": "localhost",
    "port": 3000,
    "proxyPort": 3001,
    "localOnly": true
  },
  "paperless": {
    "baseDir": "~/paperless",
    "importDir": "~/importToPaperless",
    "defaultNotebook": "Inbox"
  },
  "auth": {
    "handler": "./auth/EmptyAuth.js"
  },
  "mail": {
    "redirect_uri": "https://localhost:3000/gmail",
    "pendingLabel": "Paperless",
    "doneLabel": "Paperless/Archived",
    "importedTag": "gmail",
    "supported": false
  },
  "https": {
    "use": false
  }
}
